options {}

pipeline "parse_wgl_file" {
    procedural_stage "insert_indentation_tokens_stage"
    stage "engine/wgl/parser/classify.stage" {
        filter "engine/wgl/parser/classify.det"
    }
    stage "engine/wgl/parser/lexer.stage" {
        filter "engine/wgl/parser/lexer.det"
    }
    stage "engine/wgl/parser/syntax.stage" {
        import "det/common/indentation.det"
        parser "engine/wgl/parser/wgl_syntax.det"
    }
}
